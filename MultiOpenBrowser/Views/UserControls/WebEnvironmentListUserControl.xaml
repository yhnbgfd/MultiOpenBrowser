<UserControl
    x:Class="MultiOpenBrowser.Views.UserControls.WebEnvironmentListUserControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="800"
    Background="#FFF0F0F0"
    Loaded="UserControl_Loaded"
    mc:Ignorable="d">
    <DockPanel>
        <TabControl x:Name="TabControl_Group">
            <TabItem Header="ALL">
                <ScrollViewer>
                    <WrapPanel x:Name="StackPanel_WebEnvironmentList" Margin="5,0,5,0" />
                </ScrollViewer>
            </TabItem>
        </TabControl>
    </DockPanel>
</UserControl>
